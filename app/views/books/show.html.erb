<h1>Books detail</h1>

<table>
  <tbody>
    <% @books.each do |book| %>
    <tr>
      <%= image_tag book.get_image %>
      <td><%= image_tag %><%= link_to '/user/show', user_session_path %></td>
      <td><%=book.title %></td>
      <td><%=book.body %></td>
      <td><%=link_to 'Edit',edit_book_path(book.id)%></td>
      <td><%=link_to 'Destroy',book,method: :delete,date: {confirm:"Are you sure?"}%></td>
    </tr>
    <% end %>
  </tbody>
</table>